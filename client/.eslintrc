{
  "extends": [
    "taro"
  ],
  "globals": {
    "wx": "readonly"
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "Taro"
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx",
          ".tsx"
        ]
      }
    ],
    "jsx-quotes": [
      "error",
      "prefer-double"
    ]
  },
  "parser": "babel-eslint"
}
